<div class="d-flex justify-content-between">
  <h3>Categories</h3>
  <% if user_signed_in? %>
    <%= link_to 'New Category', new_category_path, class: "btn btn-success" %>
  <% end %>
</div>
<ul>
<% @categories.each do |cat|%>
   <%= link_to category_path(cat), class: "text-dark" do %>
    <li><%= cat.category %></li>
  <% end %>
<% end %>
</ul>